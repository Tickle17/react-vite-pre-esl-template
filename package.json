{
  "name": "react-vite-eslint-prettier-template",
  "version": "1.0.3",
  "description": "Custom React template with Vite, ESLint, Prettier, and TypeScript",
  "bin": {
    "react-vite-eslint-prettier-template": "./cli.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --fix && prettier --write .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.1"
  },
  "keywords": [
    "react",
    "template",
    "vite",
    "eslint",
    "prettier"
